{
  "name": "rsrc",
  "builds": [
    { "src": "readme.md", "use": "@now/md" },
    { "src": "license.md", "use": "@now/md" },
    { "src": "*.html", "use": "@now/static" },
    { "src": "*.js", "use": "@now/static" },
    { "src": "examples/*", "use": "@now/static" }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/readme.html"
    }
  ],
  "alias": "rsrc.klmntn.com",
  "version": 2,
  "public": true
}
